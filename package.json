{
  "name": "heinjector",
  "version": "0.0.1",
  "description": "Simple dependency injector",
  "repository": {
    "type": "git",
    "url": "https://github.com/williamthome/heinjector.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "rimraf lib && npx tsc -p tsconfig.build.json",
    "test": "npx jest --passWithNoTests --silent --noStackTrace --runInBand --detectOpenHandles",
    "test:verbose": "npx jest --passWithNoTests --runInBand --detectOpenHandles",
    "test:ci": "npm test -- --coverage",
    "lint": "npx eslint --cache --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "nodejs",
    "javascript",
    "typescript"
  ],
  "author": "William Fank Thom√©",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.1",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "lint-staged": "^10.5.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "engines": {
    "node": "12.x"
  }
}