{
  "name": "heinjector",
  "version": "0.0.1",
  "description": "Simple dependency injector",
  "repository": {
    "type": "git",
    "url": "https://github.com/williamthome/heinjector.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && npx tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "build:start": "npm run build && npm start",
    "test": "npx jest --passWithNoTests --silent --noStackTrace --runInBand --detectOpenHandles",
    "test:verbose": "npx jest --passWithNoTests --runInBand --detectOpenHandles",
    "test:unit": "npm test -- --watch \"^(.*/)*(.*\\.)*(spec)\\.ts$\"",
    "test:unit:verbose": "npm run test:verbose -- --watch \"^(.*/)*(.*\\.)*(spec)\\.ts$\"",
    "test:integration": "npm test -- --watch \"^(.*/)*(.*\\.)*(test)\\.ts$\"",
    "test:integration:verbose": "npm run test:verbose -- --watch \"^(.*/)*(.*\\.)*(test)\\.ts$\"",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:clear": "npx jest --clearCache"
  },
  "keywords": [
    "nodejs",
    "javascript",
    "typescript"
  ],
  "author": "William Fank Thom√©",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.1",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "lint-staged": "^10.5.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "module-alias": "^2.2.2"
  },
  "engines": {
    "node": "12.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
